		<function>vertical aim axis</function>
		<mouseaxis>y</mouseaxis>
	</control>
	<control>
		<function>horizontal aim axis</function>
		<mouseaxis>x</mouseaxis>
	</control>
	<control function="doubletime emote">
		<combo>
			<key>left control</key>
			<key>1</key>
		</combo>
	</control>
	<control function="get down emote">
		<combo>
			<key>left control</key>
			<key>2</key>
		</combo>
	</control>
	<control function="halt emote">
		<combo>
			<key>left control</key>
			<key>3</key>
		</combo>
	</control>
	<control function="moveout emote">
		<combo>
			<key>left control</key>
			<key>4</key>
		</combo>
	</control>
	<control function="request ammo emote">
		<combo>
			<key>left control</key>
			<key>5</key>
		</combo>
	</control>
	<control function="deploy">
		<key>b</key>
	</control>
	<control function="toggle free look">
		<key>n</key>
	</control>
	<control function="trooper lean left">
		<key>z</key>
	</control>
	<control function="trooper lean right">
		<key>c</key>
