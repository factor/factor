	</control>
	<control function="gunsight range increase">
		<key>page up</key>
	</control>
	<control function="jump signal emote">
		<combo>
			<key>left control</key>
			<key>6</key>
		</combo>
	</control>
	<control function="select next weapon">
		<key>]</key>
		<mousebutton index="1">wheelup</mousebutton>
	</control>
	<control function="select previous weapon">
		<key>[</key>
		<mousebutton index="1">wheeldown</mousebutton>
	</control>
