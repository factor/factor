            <doc xml:space="preserve">the ID of an #AtkPlug</doc>
            <type name="utf8" c:type="const gchar*"/>
          </parameter>
        </parameters>
      </method>
      <method name="is_occupied" c:identifier="atk_socket_is_occupied" version="1.30">
