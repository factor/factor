 <RULES SET="CT2" IGNORE_CASE="TRUE">
  <EOL_SPAN_REGEXP TYPE="COMMENT4" MATCH_TYPE="CONTEXT">(.{65})</EOL_SPAN_REGEXP>
 </RULES> <!-- SET="CT2" -->


