		<SEQ TYPE="NULL" DELEGATE="MAIN"> </SEQ>
		<SEQ TYPE="OPERATOR" DELEGATE="MAIN">,</SEQ>

		<SEQ TYPE="OPERATOR" DELEGATE="PROPERTIES">{</SEQ>

		<SPAN_REGEXP HASH_CHAR="lang" TYPE="KEYWORD4">
			<BEGIN>lang\s*\(</BEGIN>
			<END>)</END>
		</SPAN_REGEXP>

		<SPAN_REGEXP HASH_CHAR="lang" TYPE="KEYWORD4">
