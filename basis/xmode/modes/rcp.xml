	<RULES SET="RCP_LITERAL" ESCAPE="\" DEFAULT="LITERAL1">
		<MARK_FOLLOWING TYPE="KEYWORD3">$</MARK_FOLLOWING>
		<SEQ TYPE="LITERAL1">\</SEQ>
		<SEQ TYPE="LITERAL1">=</SEQ>
		<SEQ TYPE="LITERAL1">!</SEQ>
