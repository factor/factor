		<EOL_SPAN TYPE="COMMENT3" DELEGATE="DOC_COMMENT">///</EOL_SPAN>

		<EOL_SPAN TYPE="COMMENT2">//</EOL_SPAN>

		<EOL_SPAN TYPE="COMMENT4">///</EOL_SPAN>

		<!-- verbatim strings -->
