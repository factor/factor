<MODE>

	<PROPS>
		<PROPERTY NAME="lineComment" VALUE="#"/>
		<PROPERTY NAME="contextInsensitive" VALUE="true"/>
	</PROPS>

	<RULES HIGHLIGHT_DIGITS="TRUE" IGNORE_CASE="TRUE">
		<!-- Comments -->
		<EOL_SPAN TYPE="COMMENT2">##</EOL_SPAN>
		<EOL_SPAN TYPE="COMMENT1">#</EOL_SPAN>

		<!-- Labels -->
		<SEQ_REGEXP HASH_CHARS="ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890+-/*=()?'.&amp;" AT_LINE_START="TRUE"
			    TYPE="LABEL">[0-9A-Z+-/*=()?'.&amp;]+
		</SEQ_REGEXP>
		<!-- +/- Pseudo-labels (comments really) -->
		<SEQ_REGEXP HASH_CHARS=" +-" AT_LINE_START="TRUE" TYPE="COMMENT3">[0-9+-]+</SEQ_REGEXP>

		<!-- Octal numbers (default). -->
		<SEQ_REGEXP HASH_CHARS="+-01234567" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-7]+[ \t]</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHARS="+-01234567" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-7]+$</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHARS="+-01234567" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-7]+[,$]</SEQ_REGEXP>
		<!-- Decimal numbers specified by trailing D. -->
		<SEQ_REGEXP HASH_CHARS="+-0123456789" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-9]+D[ \t]</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHARS="+-0123456789" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-9]+D$</SEQ_REGEXP>
		<SEQ_REGEXP HASH_CHARS="+-0123456789" AT_WORD_START="TRUE" TYPE="DIGIT">[+-]*[0-9]+D[,$]</SEQ_REGEXP>

		<!-- Binary Operators -->
		<SEQ TYPE="OPERATOR">+</SEQ>
		<SEQ TYPE="OPERATOR">-</SEQ>

